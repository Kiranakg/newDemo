{
    "id": "zee_search",
    "layout": {
      "_comments": "Layout",
      "component": "collection",
      "props": {
        "className": "desktop",
        "name": "Root Collection",
        "items": [
          {
            "_comments": "Header component",
            "component": "collection",
            "stateObject": {
              "showLanguages": false
            },
            "props": {
              "className": "header",
              "style": {
                "width": "100%"
              },
              "items": [{
                "component": "collection",
                "props": {
                  "className": "header-left-content",
                  "items": [
                    {
                      "component": "button",
                      "props": {
                        "style": {},
                        "text": "Home",
                        "localText": "${home}",
                        "action": "Route_home",
                        "className": "menu_btn home_btn",
                        "icon": "Group%2053.png"
                      }
                    },
                    {
                      "component": "button",
                      "props": {
                        "style": {},
                        "text": "Shows",
                        "type": "shows",
                        "id": "114",
                        "action": "Route_menu/{type}/{id}",
                        "className": "menu_btn"
                      }
                    },
                    {
                      "component": "button",
                      "props": {
                        "style": {},
                        "text": "Movies",
                        "type": "movies",
                        "id": "119",
                        "action": "Route_menu/{type}/{id}",
                        "className": "menu_btn"
                      }
                    },
                    {
                      "component": "button",
                      "props": {
                        "style": {},
                        "text": "Series",
                        "type": "series",
                        "id": "115",
                        "action": "Route_menu/{type}/{id}",
                        "className": "menu_btn"
                      }
                    }
                  ]
                }
              },
                {
                  "component": "collection",
                  "props": {
                    "className": "header-right-content",
                    "items": [
                      {
                        "component": "button",
                        "props": {
                          "style": {},
                          "text": "Arabic",
                          "type": "arabic",
                          "id": "118",
                          "action": "Route_menu/{type}/{id}",
                          "className": "menu_btn"
                        }
                      },
                      {
                        "component": "button",
                        "props": {
                          "style": {},
                          "text": "EPG",
                          "day": "0",
                          "action": "Route_epg/{day}",
                          "className": "menu_btn"
                        }
                      },
                      {
                        "component": "button",
                        "props": {
                          "localText": "${change_language}",
                          "style": {},
                          "action": "toggleState_showLanguages",
                          "className": "menu_btn combo-btn"
                        }
                      },
                      {
                        "component": "collection",
                        "props": {
                          "role": "dialog",
                          "className": "dialog combobox popup",
                          "visibileWithState" : "showLanguages",
                          "items": [
                            {
                              "component": "button",
                              "props": {
                                "style": {},
                                "text": "Arabic",
                                "id": "Id",
                                "action": "changeLocale_ar",
                                "className": "combobox-btn"
                              }
                            },
                            {
                              "component": "button",
                              "props": {
                                "style": {},
                                "text": "English",
                                "id": "Id",
                                "action": "changeLocale_en",
                                "className": "combobox-btn"
                              }
                            },
                            {
                              "component": "button",
                              "props": {
                                "style": {},
                                "text": "Bengali",
                                "id": "Id",
                                "action": "changeLocale_bn",
                                "className": "combobox-btn"
                              }
                            }

                          ],
                          "focus" : {
                            "up": {
                              "nextFocusabledItemClassSelector": "up"
                            },
                            "down": {
                              "nextFocusabledItemClassSelector": "down"
                            },
                            "left" : {
                              "nextFocusabledItemClassSelector": "none"
                            },
                            "right": {
                              "nextFocusabledItemClassSelector": "none"
                            }
                          }
                        }
                      }
                    ]
                  }
                }]
            }
          },
          {
            "_comments": "List component",
            "component": "collection",
            "props": {
              "className": "search-list-container",
              "style": {
                "width": "100%"
              },
              "items": [
                {
                  "component": "search_bar",
                  "props": {
                    "searchRequestDelay": 2000,
                    "showSuggetionBox": false,
                    "showSuggetionGrid": true,
                    "showSoftKeyboard": true,
                    "softKeyboardOptions": {
                      "comment": "available keyboard types: custom, qwerty, abc",
                      "keyboardType": "qwerty",
                      "width": "65",
                      "margin": "2"
                    },
                    "itemPath": "data.data",
                    "title": "v_title",
                    "v_id": "v_id",
                    "action": "Route_player/program/{v_id}/{v_id}",
                    "items": [
                      {
                        "_comments": "List Bucket Items",
                        "component": "list",
                        "name": "videoList",
                        "props": {
                          "showLoader": false,
                          "style": {},
                          "className": "homeList",
                          "name": "videoList_list2",
                          "_comment": "this search result list",
                          "horizontal": true,
                          "circular": false,
                          "grid": true,
                          "isCarousel": false,
                          "items": "data",
                          "itemTitle": "",
                          "viewMore": false,
                          "type": "video"
                        },
                        "dataURL": "searchURL",
                        "itemRenderer": [
                          {
                            "component": "thumbnail",
                            "props": {
                              "name": "videoList_list2_image",
                              "src": "imagery.mobile_img",
                              "fallbackSrc": "background.png",
                              "hideFallback": true,
                              "showArrow": false,
                              "cardOverlay": false,
                              "videoId": "id",
                              "contentId": "id",
                              "type": "content_type",
                              "synopsis": "v_short_desc",
                              "title": "v_title",
                              "rating": "2.5",
                              "src_backdrop": "v_extralarge_image",
                              "clickable": true,
                              "style": {
                                "width": "100%"
                              },
                              "className": "list-item",
                              "action": "Route_details/{type}/{contentId}"
                            }
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    },
    "theme": {},
    "components": {}
  }
