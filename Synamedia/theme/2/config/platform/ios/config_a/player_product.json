                "className": "details",
                "items": [
                  {
                    "component": "collection",
                    "props": {
                      "className": "title-section",
                      "items": [
                        {
                          "component": "label",
                          "props": {
                            "text": "title",
                            "className": "content-title"
                          }
                        },
                        {
                          "component": "collection",
                          "props": {
                            "className": "rating",
                            "items": [
                              {
                                "component": "label",
                                "props": {
                                  "text": "production_year",
                                  "className": "content-data"
                                }
                              },
                              {
                                "component": "label",
                                "props": {
                                  "text": "age_rating",
                                  "className": "content-data"
                                }
                              }
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component": "collection",
                    "props": {
                      "className": "rating",
                      "items": [
                        {
                          "component": "label",
                          "props": {
                            "text": "genres",
                            "className": "content-data"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component": "collection",
                    "props": {
                      "className": "details_left",
                      "items": [
                        {
                          "component": "label",
                          "props": {
                            "text": "synopsis",
                            "className": "content-data"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "component": "label",
                    "props": {
                      "text": "${siimilar_programs}",
                      "className": "similar_contents content-details"
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "component": "list",
          "name": "similarPlayList",
          "props": {
            "style": {},
            "_comment": "player related bucket",
            "className": "content-details",
            "horizontal": true,
            "circular": false,
            "isCarousel": false,
            "items": "data"
