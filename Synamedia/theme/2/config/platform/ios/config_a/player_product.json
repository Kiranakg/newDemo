          "itemRenderer": [
            {
              "component": "collection",
              "props": {
                "className": "related-contents-section",
                "items": [
                  {
                    "component": "image",
                    "props": {
                      "hideFallback": true,
                      "forceReload": true,
                      "src": "imagery.thumbnail",
                      "imageDimension": "?d=200X200",
