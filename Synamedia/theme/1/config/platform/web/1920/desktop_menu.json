            }
          ],
          "focus": {
            "down": {
              "nextFocusabledItemClassSelector": ".carousel-list .swiper-slide-active"
            }
          }
        }
      },
      {
        "_comments": "Carousel component",

        "component": "list",
        "name": "menuNavigation",
        "props": {
          "style": {},
          "horizontal": true,
          "circular": true,
          "isCarousel": true,
          "showLoader": false,
          "className": "carousel-list",
          "items": "data.featured.playlists.0.content"
        },
        "dataURL": "menuNavigation",
        "itemRenderer": [{
          "component": "collection",
          "props": {
            "className": "carousel-item",
            "items": [{
              "component": "collection",
              "props": {
                "className": "carousel-item--content",
                "items": [{
                  "component": "label",
                  "props": {
                    "text": "title",
                    "style": {},
                    "className": "carousel-item--name"
                  }
                },
                  {
                    "component": "label",
                    "props": {
                      "text": "synopsis",
                      "style": {},
                      "className": "carousel-item--desc"
                    }
                  },
                  {
                    "component": "button",
                    "props": {
                      "style": {},
                      "text": "Play",
                      "localText": "${play}",
                      "itemId": "id",
                      "type":"content_type",
                      "videoId": "video_id",
                      "contentId": "id",
                      "action": "Route_player/{type}/{contentId}/{videoId}",
                      "className": "carousel-item--btn"
                    }
                  },
                  {
                    "component": "button",
                    "props": {
                      "style": {},
                      "text": "Details",
                      "localText": "${details}",
                      "type": "content_type",
                      "id": "id",
                      "action": "Route_details/{type}/{id}",
                      "className": "carousel-item--btn"
                    }
                  }
                ]
              }
            },
              {
                "component": "image",
                "props": {
                  "src": "imagery.backdrop",
                  "videoId": "video_id",
                  "clickable": false,
                  "style": {
                    "width": "100%",
                    "height": "100%"
                  },
                  "className": "carousel-item--image"
                }
              }

            ]
          }
        }]
      },
      {
        "_comments": "List Bucket Items",
        "component": "list",
        "name": "menuNavigation",
        "props": {
          "horizontal": false,
          "style": {},
          "items": "data.playlists"
        },
        "dataURL": "menuNavigation",
        "itemRenderer": [{
          "component": "list",
          "props": {
            "style": {},
            "_comment": "this is carousel",
            "horizontal": true,
            "circular": true,
            "isCarousel": false,
            "showLoader": false,
            "items": "content",
            "itemTitle": "title"
          },
          "itemRenderer": [{
            "component": "thumbnail",
            "props": {
              "src": "imagery.thumbnail",
              "fallbackSrc": "default.png",
              "hideFallback": true,
              "imageDimension": "?d=200X200",
              "type": "content_type",
              "synopsis": "synopsis",
              "title": "title",
              "src_backdrop": "imagery.backdrop",
              "clickable": true,
              "style": {
                "width": "100%"
              },
              "id": "id",
              "className": "swiper-item--image",
              "action": "Route_details/{type}/{id}"
            }
          }]
        }]
      }
